{
  "name": "@ronilaukkarinen/stylelint-a11y",
  "version": "1.2.4",
  "description": "Plugin for stylelint with a11y rules",
  "main": "dist/index.js",
  "repository": "https://github.com/YozhikM/stylelint-a11y.git",
  "author": "Stanislav <botev.st@gmail.com>",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "a11y"
  ],
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "prettify": "prettier '**/*.js' --write",
    "prepublish": "npm run build",
    "pretest": "npm run lint -- --fix",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "stylelint": "^14.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-jest": "^27.2.0",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "extend": ">=3.0.2",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "jest-preset-stylelint": "^4.1.1",
    "lodash": "^4.17.21",
    "parent-module": "^3.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.0.1"
  }
}
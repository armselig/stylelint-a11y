{
  "name": "@ronilaukkarinen/stylelint-a11y",
  "version": "1.2.4",
  "description": "Plugin for stylelint with a11y rules",
  "main": "dist/index.js",
  "repository": "https://github.com/ronilaukkarinen/stylelint-a11y.git",
  "author": "Stanislav <botev.st@gmail.com>",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "a11y"
  ],
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "prettify": "prettier '**/*.js' --write",
    "prepublish": "npm run build",
    "pretest": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "stylelint": "^14.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.12",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-preset-jest": "^28.0.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-stylelint": "^15.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "extend": ">=3.0.2",
    "jest": "^28.1.0",
    "jest-cli": "^28.1.0",
    "jest-preset-stylelint": "^5.0.3",
    "lodash": "^4.17.21",
    "parent-module": "^3.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "stylelint": "^14.0.1"
  }
}
